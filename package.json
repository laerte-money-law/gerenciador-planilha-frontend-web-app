{
    "name": "money-law-insurance-frontend-web-app",
    "version": "2025.1.111",
    "scripts": {
        "ng": "ng",
        "start": "ng serve ",
        "start:network": "ng serve --host $(npm run get-ip --silent) --disable-host-check ",
        "start:prod": "ng serve --configuration qa ",
        "start:qa": "ng serve --configuration prod ",
        "build:qa": "ng build --configuration qa",
        "build:prod": "ng build --configuration prod",
        "fb:deploy:qa": "firebase deploy --only hosting:qa",
        "fb:deploy:prod": "firebase deploy --only hosting:prod",
        "deploy:all": "npm-run-all deploy:qa deploy:prod",
        "deploy:qa": "npm-run-all build:qa fb:deploy:qa",
        "deploy:prod": "npm-run-all build:prod fb:deploy:prod",
        "watch": "ng build --watch --configuration development"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.3.12",
        "@angular/cdk": "^17.3.10",
        "@angular/common": "^17.3.0",
